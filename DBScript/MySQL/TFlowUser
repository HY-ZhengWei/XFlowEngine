CREATE TABLE TFlowUser
(
  userID                       VARCHAR(64)        NOT NULL
 ,flowID                       VARCHAR(64)        NOT NULL
 ,createTime                   DATETIME           NOT NULL
 ,updateTime                   DATETIME           NOT NULL
);


ALTER TABLE TLog                                 ADD PRIMARY KEY (userID ,flowID);

ALTER TABLE TLog                                 ADD INDEX (userID ,flowState);

ALTER TABLE TLog                                 ADD INDEX (createTime);
  
ALTER TABLE TLog                                 ADD INDEX (updateTime);