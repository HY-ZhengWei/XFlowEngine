<?xml version="1.0" encoding="UTF-8"?>

<config>

	<import name="sqls"            class="java.util.ArrayList" />
	<import name="sqlGroup"        class="org.hy.common.xml.plugins.XSQLGroup" />
	<import name="sql"             class="org.hy.common.xml.XSQL" />



    <!-- 工作流过程表模块的相关SQL -->
	<sqls>
	
		<sql id="XSQL_XFlow_Process_Insert"> 
		
			<dataSourceGroup ref="DSG_XFlow" />
			
			<content>
				<![CDATA[
				INSERT  INTO Process
				       (
				        processID
				       ,workID
				       ,splitProcessID
				       ,processNo
				       ,currentActivityID
				       ,currentActivityName
				       ,previousProcessID
				       ,previousActivityID
				       ,previousActivityName
				       ,nextProcessID
				       ,nextActivityID
				       ,nextActivityName
				       ,createrID
				       ,creater
				       ,createOrgID
				       ,createOrg
				       ,createTime
				       ,limitUserID
				       ,limitOrgID
				       ,limitTime
				       ,operateTime
				       ,operateTimeLen
				       ,operateTypeID
				       ,operateType
				       ,operateUserID
				       ,operateUser
				       ,operateOrgID
				       ,operateOrg
				       ,operateFiles
				       ,operateDatas
				       ,infoComment
				       )
				VALUES (
				        ':process.processID'
				       ,':process.workID'
				       ,':process.splitProcessID'
				       , :process.processNo
				       ,':process.currentActivityID'
				       ,':process.currentActivityName'
				       ,':process.previousProcessID'
				       ,':process.previousActivityID'
				       ,':process.previousActivityName'
				       ,':process.nextProcessID'
				       ,':process.nextActivityID'
				       ,':process.nextActivityName'
				       ,':process.createrID'
				       ,':process.creater'
				       ,':process.createOrgID'
				       ,':process.createOrg'
				       ,':process.createTime'
				       ,':process.limitUserID'
				       ,':process.limitOrgID'
				       ,':process.limitTime'
				       ,':process.operateTime'
				       , :process.operateTimeLen
				       ,':process.operateTypeID'
				       ,':process.operateType'
				       ,':process.operateUserID'
				       ,':process.operateUser'
				       ,':process.operateOrgID'
				       ,':process.operateOrg'
				       ,':process.operateFiles'
				       ,':process.operateDatas'
				       ,':process.infoComment'
				       )
				]]>
			</content>
			
		</sql>
		
	</sqls>
	
</config>